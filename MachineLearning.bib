	Issn = {1053-5888},
	Journal = {IEEE Signal Processing Magazine},
	Keywords = {Deep Learning - Speech},
	Month = {Nov},
	Number = {6},
	Pages = {82-97},
	Title = {Deep Neural Networks for Acoustic Modeling in Speech Recognition: The Shared Views of Four Research Groups},
